{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "@/components/*": ["src/components/*"],
      "@/icons/*": ["src/icons/*"],
      "@/lib/*": ["src/lib/*"],
      "@/interfaces/*": ["src/interfaces/*"],
      "@/layouts/*": ["src/layouts/*"],
      "@/helpers/*": ["src/helpers/*"],
      "@/stores/*": ["src/stores/*"],
    },
  }
}